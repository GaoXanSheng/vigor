{% extends '../layout.njk' %}

{% block main %} 
<h4 class="text-center">消息设置</h4>
<div class="alert alert-success hidden" role="alert" ng-show="updated">更新成功!</div>
    <form class="form-horizontal message" method="post"  enctype="multipart/form-data">
      <div class="form-group">
        <label for="aes" class="col-sm-2 control-label">Encoding AES Key</label>
       <div class="col-sm-10">
        <input type="text" name="aes" ng-model="message.aes" class="form-control" id="aes" placeholder="Encoding AES Key"  value="{{data.aes}}">
       </div>
      </div>
      <hr/>
  <div class="text-center">
      <button type="submit" class="btn btn-danger">提交</button>
  </div>
      </form>
{% endblock %}